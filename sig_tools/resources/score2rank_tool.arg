-
  name: score_file
  default: ''
  help: 'Score matrix in GCT(X) format'

-
  name: --out
  default: '.'
  help: 'output folder.'

-
  name: --row_space
  default: 'full'
  help: 'Feature space. Valid options are {lm, [full], bing, custom}'

-
  name: --rid
  default: ''
  help: 'List of query ids to use. Default is all.'

-
  name: --score2rank_direc
  default: 'descend'
  help: Sort order of scores.
        Valid options are {[descend], ascend}.
